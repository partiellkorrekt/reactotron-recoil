    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "5.0.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.2.1",
    "reactotron-core-client": "2.8.10",
    "recoil": "^0.1.2",
    "rollup": "2.36.2",
